<CsoundSynthesizer><CsOptions>--env:SSDIR+=../SourceMaterials -odac</CsOptions><CsInstruments>;Example by Alex Hofmanninstr 1iAmp =	p4iFrq = p5printf_i   "amp = %.3f\n", 1, iFrqaSin      oscils    0dbfs/iAmp, iFrq, 0          out       aSinendin
instr 2	iAmp =	p4	iFrq = p5		iNum = p6	kFlag init 1	kOscCount init 1	if kFlag == 1 then		until kOscCount>iNum do			event "i", 1, 0, 1, iAmp, iFrq*kOscCount			kOscCount = kOscCount + 1		enduntil 		kFlag = 0	endifendin
instr 3	kFrqArr[] array 1.5, 1.7, 2.3, 3.4, 5.5, 5.2, 5.6, 5.9, 8.2, 9.8	iAmp =	p4	iFrq = p5		iNum = p6	kOscCount init 1	kCat metro 0.5	if kCat == 1 then		until kOscCount>iNum do			event "i", 1, 0, 1, iAmp*kOscCount, iFrq*kFrqArr[kOscCount-1]			kOscCount = kOscCount + 1		enduntil 		;event "i", 1, 0, 1, 1, 440		kOscCount = 1	endifendin</CsInstruments><CsScore>;		amp		frq		number/*i 1 0 1 1		440i 1 2 1 1		880i 1 4 1 1		440i 1 6 1 1		660i 2 0 1 1		440		3i . 2 1 1		.		5i . 4 1 1		.		10i . 6 1 1		.		6*/
i 3 0 10 1		440		10</CsScore></CsoundSynthesizer>